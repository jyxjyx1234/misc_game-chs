{
  "injector": {
    "enable": true,
    "print_loaded_modules": false
  },
  "allocate_console": false,
  "locale_emulator": {
    "charset": 128,
    "codepage": 932,
    "enable": true,
    "locale": 1041,
    "timezone": "Tokyo Standard Time",
    "wait_for_exit": false
  },
  "font_manager": {
    "enable": true,
    "resource_files": [
      "delta_Mahou_font_tra.ttf"
    ],
    "spoof_enumeration": {
      "enable": false,
      "filter_pitch_and_family": 0,
      "filter_charset": 1,
      "spoof_charset": 128
    },
    "spoof_creation": {
      "enable": true,
      "override_face": "delta_Mahou2_font",
      "override_charset": 128
    }
  },
  "window_manager": {
    "enable": true,
    "overwrite_title": "アルテミナ2戦え！アポローグ_Sakura机翻 by jyx",
    "process_existing_windows": true
  }
}